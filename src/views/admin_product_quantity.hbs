<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">Product ID</th>
            <th scope="col">Product Name</th>
            <th scope="col">Quantity</th>
        </tr>
    </thead>
    {{#each products }}
    <tbody>
        <tr>
            <td>{{ product_ID }}</td>
            <td>{{ product_name }}</td>
            <td>{{ quantity }}</td>
        </tr>
    </tbody>
    {{/each}}
</table>
